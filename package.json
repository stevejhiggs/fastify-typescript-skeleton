{
  "name": "fastify-example",
  "version": "1.0.0",
  "description": "",
  "main": "dist/listener.js",
  "type": "module",
  "scripts": {
    "start": "bun ./index.ts | pino-pretty",
    "start:production": "bun ./index.ts",
    "test": "LOG_LEVEL=warn bun test",
    "test:ci": "bun run test --coverage",
    "typecheck": "bun -b tsc --noEmit",
    "lint:ci": "bun -b biome check ./src",
    "lint": "bun -b run lint:ci --write",
    "setup-commit-hooks": "bun -b husky",
    "pre-commit": "bun -b lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevejhiggs/fastify-typescript-skeleton.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevejhiggs/fastify-typescript-skeleton/issues"
  },
  "homepage": "https://github.com/stevejhiggs/fastify-typescript-skeleton#readme",
  "dependencies": {
    "@fastify/helmet": "^13.0.1",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "fastify": "^5.6.0",
    "fastify-type-provider-zod": "^6.0.0",
    "pino": "^9.9.5",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/bun": "^1.2.22",
    "@types/node": "^24.5.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "pino-pretty": "^13.1.1",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "bun -b biome check --write --no-errors-on-unmatched"
    ]
  },
  "packageManager": "bun@1.2.22"
}
